{
  "deps": [
    {
      "name": "luatexja"
    }
  ],
  "macros": [
    {
      "name": "ltjruby",
      "arg": {
        "format": "{}{}",
        "snippet": "ltjruby{${1:親|${3:文}|${4:字}}{${2:おや}|${5:も}|じ}}"
      }
    },
    {
      "name": "ltjruby",
      "arg": {
        "format": "[]{}",
        "snippet": "ltjruby[${2:options%keyvals}]{${1:親|${3:文}|字}}",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ruby",
      "arg": {
        "format": "{}{}",
        "snippet": "ruby{${1:親|${3:文}|${4:字}}{${2:おや}|${5:も}|じ}}"
      }
    },
    {
      "name": "ruby",
      "arg": {
        "format": "[]{}",
        "snippet": "ruby[${2:options%keyvals}]{${1:親|${3:文}|字}}",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ltjsetruby",
      "arg": {
        "format": "{}",
        "snippet": "ltjsetruby{${1:options%keyvals}}",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ltjkenten"
    },
    {
      "name": "ltjkenten",
      "arg": {
        "format": "[]",
        "snippet": "ltjkenten[${1:options%keyvals}]",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "kenten"
    },
    {
      "name": "kenten",
      "arg": {
        "format": "[]",
        "snippet": "kenten[${1:options%keyvals}]",
        "keys": [
          "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\ltjruby,\\ruby,\\ltjsetruby,\\ltjkenten,\\kenten": [
      "pre=${1:real}",
      "post=${1:real}",
      "mode=",
      "intrude_jfmgk",
      "intrude_kanjiskip",
      "intrude_xkanjiskip",
      "stretchruby={${1:left}}{${2:middle}}{${3:right}}",
      "stretch={${1:left}}{${2:middle}}{${3:right}}",
      "stretchbol={${1:left}}{${2:middle}}{${3:right}}",
      "stretcheol={${1:left}}{${2:middle}}{${3:right}}",
      "maxmargin=${1:real}",
      "size=${1:real}",
      "intergap=${1:real}",
      "rubysmash",
      "ybaseheight=${1:real}",
      "tbaseheight=${1:real}",
      "baseheight=${1:real}",
      "yrubydepth=${1:real}",
      "trubydepth=${1:real}",
      "rubydepth=${1:real}",
      "kenten=${1:command}",
      "fontcmd=${1:command}",
      "always_highlight_protrusion",
      "naka",
      "kata"
    ],
    "\\ltjsetparameter,\\ltjglobalsetparameter": [
      "rubypreintrusion={${1:chr_code,pre_int}}",
      "rubypostintrusion={${1:chr_code,post_int}}"
    ],
    "\\ltjgetparameter": [
      "rubypreintrusion",
      "rubypostintrusion"
    ]
  },
  "args": []
}