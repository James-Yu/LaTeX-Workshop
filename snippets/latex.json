		"description": "tikzpicture"
	},
	"set font size": {
		"prefix": "fontsize",
		"body": "${1|\\Huge,\\huge,\\LARGE,\\Large,\\large,\\normalsize,\\small,\\footnotesize,\\scriptsize,\\tiny|}",
		"description": "Select a font size"
	},
	"textnormal": {
		"prefix": "FNO",
