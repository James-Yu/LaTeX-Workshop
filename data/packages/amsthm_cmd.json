{
  "newtheorem*{}{}": {
    "command": "newtheorem*{envname}{caption}",
    "package": "amsthm",
    "snippet": "newtheorem*{${1:envname}}{${2:caption}}"
  },
  "newtheorem*{}{}[]": {
    "command": "newtheorem*{envname}{caption}[within]",
    "package": "amsthm",
    "snippet": "newtheorem*{${1:envname}}{${2:caption}}[${3:within}]"
  },
  "newtheorem*{}[]{}": {
    "command": "newtheorem*{envname}[numberedlike]{caption}",
    "package": "amsthm",
    "snippet": "newtheorem*{${1:envname}}[${2:numberedlike}]{${3:caption}}"
  },
  "theoremstyle{}": {
    "command": "theoremstyle{style}",
    "package": "amsthm",
    "snippet": "theoremstyle{${1:style}}"
  },
  "swapnumbers": {
    "command": "swapnumbers",
    "package": "amsthm",
    "snippet": "swapnumbers"
  },
  "newtheoremstyle{}{}{}{}{}{}{}{}{}": {
    "command": "newtheoremstyle{stylename}{spaceabove}{spacebelow}{bodyfont}{indentamt}{headfont}{headpunct}{headspace}{headspec}",
    "package": "amsthm",
    "snippet": "newtheoremstyle{${1:stylename}}{${2:spaceabove}}{${3:spacebelow}}{${4:bodyfont}}{${5:indentamt}}{${6:headfont}}{${7:headpunct}}{${8:headspace}}{${9:headspec}}"
  },
  "qedsymbol": {
    "command": "qedsymbol",
    "package": "amsthm",
    "snippet": "qedsymbol"
  },
  "qedhere": {
    "command": "qedhere",
    "package": "amsthm",
    "snippet": "qedhere"
  },
  "qed": {
    "command": "qed",
    "package": "amsthm",
    "snippet": "qed"
  }
}