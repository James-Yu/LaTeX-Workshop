{
  "so{}": {
    "command": "so{text to add letter spacing}",
    "package": "soul",
    "snippet": "so{${1:text to add letter spacing}}"
  },
  "caps{}": {
    "command": "caps{small caps text}",
    "package": "soul",
    "snippet": "caps{${1:small caps text}}"
  },
  "ul{}": {
    "command": "ul{text to underline}",
    "package": "soul",
    "snippet": "ul{${1:text to underline}}"
  },
  "st{}": {
    "command": "st{text to strikeout}",
    "package": "soul",
    "snippet": "st{${1:text to strikeout}}"
  },
  "hl{}": {
    "command": "hl{text to highlight}",
    "package": "soul",
    "snippet": "hl{${1:text to highlight}}"
  },
  "soulaccent{}": {
    "command": "soulaccent{accent}",
    "package": "soul",
    "snippet": "soulaccent{${1:accent}}"
  },
