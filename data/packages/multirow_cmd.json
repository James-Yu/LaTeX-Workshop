{
  "multirow{}{}{}": {
    "command": "multirow{number of rows}{*}{text}",
    "package": "multirow",
    "snippet": "multirow{${1:number of rows}}{${2:*}}{${3:text}}"
  },
  "multirow{}[]{}[]{}": {
    "command": "multirow{number of rows}[bigstruts]{width}[fixup]{text}",
    "package": "multirow",
    "snippet": "multirow{${1:number of rows}}[${2:bigstruts}]{${3:width}}[${4:fixup}]{${5:text}}"
  }
}
