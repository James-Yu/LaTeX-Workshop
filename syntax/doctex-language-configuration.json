{
	"comments": {
		"lineComment": "%^^A"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["\\begin{", ""],
		["\\left(", ""],
		["\\left[", ""],
		["\\left{", ""],
		["\\bigl(", ""],
		["\\bigl[", ""],
		["\\bigl{", ""],
		["\\Bigl(", ""],
		["\\Bigl[", ""],
		["\\Bigl{", ""],
		["\\biggl(", ""],
		["\\biggl[", ""],
		["\\biggl{", ""],
		["\\Biggl(", ""],
		["\\Biggl[", ""],
		["\\Biggl{", ""],
		["\\(", ""],
		["\\[", ""],
		["\\{", ""],
		["@(", ""],
		["@[", ""],
		["@{", ""],
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["`", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "'"],
		["$", "$"]
	],
	"indentationRules": {
		"increaseIndentPattern": "(\\\\begin{(?!(document|macrocode)))|(\\\\documentclass)",
		"decreaseIndentPattern": "(\\\\end{)|(\\\\begin{document})"
	},
	"folding": {
		"markers": {
			"start": "^\\s*%\\s*#?region\\b.*",
			"end": "^\\s*%\\s*#?endregion\\b.*"
		}
	}
}
