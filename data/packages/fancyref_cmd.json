{
  "fref{}": {
    "command": "fref{label}",
    "package": "fancyref",
    "snippet": "fref{${1:label}}"
  },
  "Fref{}": {
    "command": "Fref{label}",
    "package": "fancyref",
    "snippet": "Fref{${1:label}}"
  },
  "fref[]{}": {
    "command": "fref[format]{label}",
    "package": "fancyref",
    "snippet": "fref[${2:format}]{${1:label}}"
  },
  "Fref[]{}": {
    "command": "Fref[format]{label}",
    "package": "fancyref",
    "snippet": "Fref[${2:format}]{${1:label}}"
  },
  "fancyrefchangeprefix{}{}": {
    "command": "fancyrefchangeprefix{macro}{preﬁx}",
    "package": "fancyref",
    "snippet": "fancyrefchangeprefix{${1:macro}}{${2:preﬁx}}"
  },
  "fancyrefaddcaptions{}{}": {
    "command": "fancyrefaddcaptions{language}{stringdefs}",
    "package": "fancyref",
    "snippet": "fancyrefaddcaptions{${1:language}}{${2:stringdefs}}"
  },
  "frefformat{}{}{}": {
    "command": "frefformat{format}{preﬁxmacro}{output}",
    "package": "fancyref",
    "snippet": "frefformat{${1:format}}{${2:preﬁxmacro}}{${3:output}}"
  },
  "Frefformat{}{}{}": {
    "command": "Frefformat{format}{preﬁxmacro}{output}",
    "package": "fancyref",
    "snippet": "Frefformat{${1:format}}{${2:preﬁxmacro}}{${3:output}}"
  }
}
