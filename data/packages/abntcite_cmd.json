{
  "bibliographystyle{}": {
    "command": "bibliographystyle{style}",
    "package": "abntcite",
    "snippet": "bibliographystyle{${1:style}}"
  },
  "cite{}": {
    "command": "cite{rotulo}",
    "package": "abntcite",
    "snippet": "cite{${1:rotulo}}"
  },
  "citeonline{}": {
    "command": "citeonline{rotulo}",
    "package": "abntcite",
    "snippet": "citeonline{${1:rotulo}}"
  },
  "citeyear{}": {
    "command": "citeyear{rotulo}",
    "package": "abntcite",
    "snippet": "citeyear{${1:rotulo}}"
  },
  "citeauthor{}": {
    "command": "citeauthor{rotulo}",
    "package": "abntcite",
    "snippet": "citeauthor{${1:rotulo}}"
  },
  "citeauthoronline{}": {
    "command": "citeauthoronline{rotulo}",
    "package": "abntcite",
    "snippet": "citeauthoronline{${1:rotulo}}"
  },
  "leftovercite": {
    "command": "leftovercite",
    "package": "abntcite",
    "snippet": "leftovercite"
  },
  "rightovercite": {
    "command": "rightovercite",
    "package": "abntcite",
    "snippet": "rightovercite"
  },
  "citeoptions{}": {
    "command": "citeoptions{options}",
    "package": "abntcite",
    "snippet": "citeoptions{${1:options}}"
  },
  "bibtextitlecommand": {
    "command": "bibtextitlecommand",
    "package": "abntcite",
    "snippet": "bibtextitlecommand"
  },
  "bibtextitlecommand{}{}": {
    "command": "bibtextitlecommand{entry}{text}",
    "package": "abntcite",
    "snippet": "bibtextitlecommand{${1:entry}}{${2:text}}"
  }
}