{
  "todo{}": {
    "command": "todo{text}",
    "package": "todonotes",
    "snippet": "todo{${1:text}}"
  },
  "todo[]{}": {
    "command": "todo[options]{text}",
    "package": "todonotes",
    "snippet": "todo[${2:options}]{${1:text}}"
  },
  "listoftodos": {
    "command": "listoftodos",
    "package": "todonotes",
    "snippet": "listoftodos"
  },
  "listoftodos[]": {
    "command": "listoftodos[caption]",
    "package": "todonotes",
    "snippet": "listoftodos[${1:caption}]"
  },
  "missingfigure{}": {
    "command": "missingfigure{text}",
    "package": "todonotes",
    "snippet": "missingfigure{${1:text}}"
  },
  "missingfigure[]{}": {
    "command": "missingfigure[options]{text}",
    "package": "todonotes",
    "snippet": "missingfigure[${2:options}]{${1:text}}"
  }
}