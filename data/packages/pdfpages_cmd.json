{
  "includepdf[]{}": {
    "command": "includepdf[options]{filename}",
    "snippet": "includepdf[${2:options}]{${1:filename}}"
  },
  "includepdf": {
    "command": "includepdf",
    "snippet": "includepdf{${1:filename}}"
  },
  "includepdfmerge[]{}": {
    "command": "includepdfmerge[options]{file-list}",
    "snippet": "includepdfmerge[${2:options}]{${1:file-list}}"
  },
  "includepdfmerge": {
    "command": "includepdfmerge",
    "snippet": "includepdfmerge{${1:file-list}}"
  },
  "includepdfset": {
    "command": "includepdfset",
    "snippet": "includepdfset{${1:global options}}"
  }
}