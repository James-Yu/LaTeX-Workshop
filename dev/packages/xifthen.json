  },
  "macros": {
    "isnamedefined{}": {
      "snippet": "isnamedefined{${1:command name}}"
    },
    "isempty{}": {
      "snippet": "isempty{${1:content}}"
    },
    "isequivalentto{}{}": {
      "snippet": "isequivalentto{${1:command1}}{${2:command2}}"
    },
    "isin{}{}": {
      "snippet": "isin{${1:substring}}{${2:string}}"
    },
    "endswith{}{}": {
      "snippet": "endswith{${1:string}}{${2:substring}}"
    },
    "cnttest{}{}{}": {
      "snippet": "cnttest{${1:counter-expr1}}{${2:comparison}}{${3:counter-expr2}}"
    },
    "dimtest{}{}{}": {
      "snippet": "dimtest{${1:dimen-expr1}}{${2:comparison}}{${3:dimen-expr2}}"
    },
    "newtest{}{}": {
      "snippet": "newtest{${1:command}}{${2:test-expr}}"
    },
    "newtest{}[]{}": {
      "snippet": "newtest{${1:command}}[${2:args}]{${3:test-expr}}"
    }
  },
