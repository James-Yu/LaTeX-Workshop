{
  "newfloat{}{}{}[]": {
    "command": "newfloat{type}{placement}{ext}[within]",
    "package": "float",
    "snippet": "newfloat{${1:type}}{${2:placement}}{${3:ext}}[${4:within}]"
  },
  "floatstyle{}": {
    "command": "floatstyle{defaultstyle}",
    "package": "float",
    "snippet": "floatstyle{${1:defaultstyle}}"
  },
  "floatname{}{}": {
    "command": "floatname{type}{name}",
    "package": "float",
    "snippet": "floatname{${1:type}}{${2:name}}"
  },
  "floatplacement{}{}": {
    "command": "floatplacement{type}{default placement specifier}",
    "package": "float",
    "snippet": "floatplacement{${1:type}}{${2:default placement specifier}}"
  },
  "restylefloat{figure or table": {
    "command": "restylefloat{figure or table",
    "package": "float",
    "snippet": "restylefloat{figure or table"
  },
  "listof{}{}": {
    "command": "listof{type}{ title}",
    "package": "float",
    "snippet": "listof{${1:type}}{${2: title}}"
  }
}
