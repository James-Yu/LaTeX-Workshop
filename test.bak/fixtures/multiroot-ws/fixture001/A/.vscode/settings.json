{
	"latex-workshop.latex.recipe.default": "latexmk",
	"latex-workshop.latex.tools": [
		{
			"name": "latexmk",
			"command": "latexmk",
			"args": [
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"-pdf",
				"-outdir=%OUTDIR%",
				"-jobname=wsA",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "fake",
			"command": "touch",
			"args": [
				"%DIR%/fake.pdf"
			]
		}
	]
}