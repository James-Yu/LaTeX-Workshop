{
  "addplot": {
    "command": "addplot",
    "package": "pgfplots",
    "snippet": "addplot"
  },
  "addplot{};": {
    "command": "addplot {%|};",
    "package": "pgfplots",
    "snippet": "addplot {${1:%|}};"
  },
  "addplottable[]{};": {
    "command": "addplot table[x=%<column header%>,y=%<column header%>] {%<file%>};",
    "package": "pgfplots",
    "snippet": "addplot table[${2:x=%<column header%>,y=%<column header%>}] {${1:%<file%>}};"
  },
  "addplotcoordinates{};": {
    "command": "addplot coordinates {%|};",
    "package": "pgfplots",
    "snippet": "addplot coordinates {${1:%|}};"
  },
  "addplotgnuplot{};": {
    "command": "addplot gnuplot {%<gnuplot code%>};",
    "package": "pgfplots",
    "snippet": "addplot gnuplot {${1:%<gnuplot code%>}};"
  },
  "addplotfile{};": {
    "command": "addplot file {%<file%>};",
    "package": "pgfplots",
    "snippet": "addplot file {${1:%<file%>}};"
  },
  "legend": {
    "command": "legend",
    "package": "pgfplots",
    "snippet": "legend"
  },
  "pgfplotsset{}": {
    "command": "pgfplotsset{compat=%<version number%>,%<other settings%>}",
    "package": "pgfplots",
    "snippet": "pgfplotsset{${1:compat=%<version number%>,%<other settings%>}}"
  }
}