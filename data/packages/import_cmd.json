{
  "import": {
    "command": "import",
    "snippet": "import{${1:full path}}{${2:file}}"
  },
  "import*": {
    "command": "import*",
    "snippet": "import*{${1:full path}}{${2:file}}"
  },
  "subimport": {
    "command": "subimport",
    "snippet": "subimport{${1:relative path}}{${2:file}}"
  },
  "subimport*": {
    "command": "subimport*",
    "snippet": "subimport*{${1:relative path}}{${2:file}}"
  },
  "includefrom": {
    "command": "includefrom",
    "snippet": "includefrom{${1:full path}}{${2:file}}"
  },
  "includefrom*": {
    "command": "includefrom*",
    "snippet": "includefrom*{${1:full path}}{${2:file}}"
  },
  "subincludefrom": {
    "command": "subincludefrom",
    "snippet": "subincludefrom{${1:relative path}}{${2:file}}"
  },
  "subincludefrom*": {
    "command": "subincludefrom*",
    "snippet": "subincludefrom*{${1:relative path}}{${2:file}}"
  }
}